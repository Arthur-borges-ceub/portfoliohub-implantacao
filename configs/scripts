# mock_drive_upload.py

def upload_to_drive_mock():
    print("[MOCK] Iniciando upload de arquivo para o Google Drive...")
    print("[MOCK] Arquivo 'relatorio_final.pdf' enviado para a pasta 'PortfolioHUB'.")
    print("[MOCK] Permissões de compartilhamento aplicadas (leitura para avaliador).")

if __name__ == "__main__":
    upload_to_drive_mock()


